{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "no-empty": false,
    "no-console": {
      "severity": "error",
      "options": ["log", "debug", "info"]
    }
  },
  "linterOptions": {
    "exclude": [
      "node_modules/**",
      "dist/**"
    ]
  }
}
